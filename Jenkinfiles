pipeline {
    agent any

   environment{
    PATH="/opt/maven/bin:$PATH"

    stages {
        stage('Git Clone') {
            steps {
                git branch: 'main', url: 'https://github.com/BharathKumarS23121995/irctcweb.git'
            }
        }

        stage('Build') {
            steps {
                sh 'mvn clean install'
            }
        }
    }
}
