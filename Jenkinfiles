pipeline{
    agent any
    environment{
    PATH="opt/maven/bin:$PATH"
    }
    stages{
        stage ('git clone'){
            steps{
               git url: 'https://github.com/BharathKumarS23121995/irctcweb.git', branch: 'main' 
            }
        }
        stage ('build'){
            steps{
                
                sh 'mvn clean install'
            }
        }
    }
    
    
}
